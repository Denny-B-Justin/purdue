SELECT
    Date,
    TargetCountry,
    TargetCountryCode,
    ReceivingCountry,
    ReceivingCountryCode,
    Action,
    NumArticles,
    AvgTone,
    AvgGoldsteinScale
FROM
    gdelt-exploration-419814.gdelt_sample.cross_country_events
WHERE
    ReceivingCountry != 1               # '1' for US, not '1' for other countries
    AND TargetCountry = 1             # '1' for US, not '1' for other countries
    AND TargetCountryCode = 1          # '1' for government, '2' for business
    AND ReceivingCountryCode = 2       # '1' for government, '2' for business

